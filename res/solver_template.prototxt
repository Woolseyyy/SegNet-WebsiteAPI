net: "<%=train_prototxt%>"  		# Change this to the absolute path to your model file
test_initialization: false
test_iter: 1
test_interval: 10000000
base_lr: 0.003
lr_policy: "step"
gamma: 1.0
stepsize: 10000000
display: 20
momentum: 0.3
max_iter: 130000
weight_decay: 0.005
snapshot: 1000
snapshot_prefix: "<%=result_dir%>"  	# Change this to the absolute path to where you wish to output solver snapshots
solver_mode: GPU
